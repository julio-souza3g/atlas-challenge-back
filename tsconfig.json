{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",                               
    "esModuleInterop": true,  
    "allowJs": true,
    "skipLibCheck": true,
    "outDir": "./dist",  
    "rootDir": "./src",
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,  
    "paths": {
      "@services/*": ["./services"],
    }                             
  }
}
